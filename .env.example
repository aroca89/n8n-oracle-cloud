# Configuración de base de datos PostgreSQL
POSTGRES_PASSWORD=tu-password-super-seguro-aqui

# Configuración de n8n
N8N_USER=admin
N8N_PASSWORD=tu-password-n8n-seguro
N8N_ENCRYPTION_KEY=clave-de-encriptacion-de-32-caracteres-hex

# Configuración de dominio
# Cambiar por tu dominio real (sin HTTP/HTTPS si no tienes SSL aún)
DOMAIN_URL=https://tu-dominio.com

# Configuración de email SMTP (opcional)
# Para notificaciones y workflows que envían emails
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=tu-email@gmail.com
SMTP_PASS=tu-app-password-de-gmail

# Configuración avanzada (opcional)
# N8N_LOG_LEVEL=info
# N8N_METRICS=true
# GENERIC_TIMEZONE=Europe/Madrid